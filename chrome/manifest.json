{
  "name": "My New Tab",
  "description": "Customize new tab with simplicity",
  "version": "1.0",
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/candle_16.png",
    "32": "icons/candle_32.png",
    "48": "icons/candle_48.png",
    "128": "icons/candle_128.png"
  },
  "permissions": ["storage"]
}
